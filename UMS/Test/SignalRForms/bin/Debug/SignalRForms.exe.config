  <system.serviceModel>
    <bindings>
      <customBinding>
        <binding name="CustomBinding_IAAService" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <textMessageEncoding messageVersion="Soap12" />
          <httpTransport />
        </binding>
        <binding name="CustomBinding_ISendUMSMessage">
          <textMessageEncoding messageVersion="Soap12" />
          <httpTransport />
        </binding>
        <binding name="CustomBinding_IAAService_Secure" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <textMessageEncoding maxReadPoolSize="2147483647" maxWritePoolSize="2147483647" messageVersion="Soap12">
            <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647" maxArrayLength="2147483647" maxBytesPerRead="2147483647" />
          </textMessageEncoding>
          <httpsTransport maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647" authenticationScheme="Negotiate" keepAliveEnabled="true" maxBufferSize="2147483647" transferMode="Buffered" />
        </binding>
        <binding name="Secure">
          <textMessageEncoding messageVersion="Soap12" />
          <httpsTransport />
        </binding>
      </customBinding>
    </bindings>



    <client>
      <endpoint address="https://localhost/SendUMSMessage/SendUMSMessage.svc" binding="customBinding" bindingConfiguration="CustomBinding_IAAService" contract="SendUMSMessage_NS.ISendUMSMessage" name="" />
